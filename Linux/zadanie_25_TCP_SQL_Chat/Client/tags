main	Client.cpp	/^int main()$/;"	f
message	ClientManager.h	/^    char message[MESSAGE_BUFFER];$/;"	m	class:ConnectManager
servaddr	ClientManager.h	/^    struct sockaddr_in servaddr;$/;"	m	class:ConnectManager	typeref:struct:ConnectManager::sockaddr_in
