LDFLAGS	Makefile	/^LDFLAGS=$/;"	m
MESSAGE_BUFFER	ClientManager.h	23;"	d
OBJECTS	Makefile	/^OBJECTS=$(SOURCES:.cpp=.o)$/;"	m
PORT	ClientManager.h	24;"	d
Receive	ClientManager.cpp	/^void ConnectManager::Receive(){   $/;"	f	class:ConnectManager
